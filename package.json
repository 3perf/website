{
  "name": "3perf.com",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.3",
    "@types/lodash": "^4.14.154",
    "aws-lambda": "^1.0.6",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "fast-xml-parser": "^3.17.3",
    "gatsby": "^2.24.65",
    "gatsby-link": "^2.4.14",
    "gatsby-react-router-scroll": "^3.0.13",
    "invariant": "^2.2.4",
    "lodash": "^4.17.15",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.0",
    "polished": "^3.6.4",
    "preact": "^10.4.4",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "^5.1.9",
    "prismjs": "^1.20.0",
    "query-string": "^6.12.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.0",
    "react-script-tag": "^1.1.2",
    "react-twitter-widgets": "^1.7.1",
    "strip-indent": "^3.0.0",
    "styled-components": "^5.1.1",
    "ts-assert-exists": "^1.0.4",
    "webpack": "^4.43.0"
  },
  "scripts": {
    "install-build-deps": "pip3 install fonttools brotli zopfli || pip install fonttools brotli zopfli",
    "build-gatsby": "gatsby build",
    "optimize-fonts": "yarn optimize-fonts:landings && yarn optimize-fonts:content",
    "optimize-fonts:landings": "cd public && find . -type f -name '*.html' | grep -v -E '(blog|gold|talks)' | xargs subfont --root . --canonical-root https://3perf.com/ --formats woff2 --in-place --inline-css",
    "optimize-fonts:content": "cd public && find . -type f -name '*.html' | grep -E '(blog|gold|talks)' | xargs subfont --root . --canonical-root https://3perf.com/ --formats woff2 --in-place --inline-css",
    "build": "yarn install-build-deps && yarn build-gatsby && yarn optimize-fonts",
    "develop": "cross-env GATSBY_MAILCHIMP_3PERF_SUBSCRIBERS=FAKE_URL gatsby develop",
    "build-locally": "cross-env GATSBY_MAILCHIMP_3PERF_SUBSCRIBERS=FAKE_URL yarn build",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-object-assign": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/aws-lambda": "^8.10.53",
    "@types/cheerio": "^0.22.18",
    "@types/classnames": "^2.2.10",
    "@types/invariant": "^2.2.33",
    "@types/lossless-json": "^1.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/prop-types": "^15.5.2",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-mailchimp-subscribe": "^2.0.1",
    "@types/styled-components": "^5.1.3",
    "@weknow/gatsby-remark-twitter": "^0.2.3",
    "babel-plugin-styled-components": "^1.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-lodash": "^3.3.2",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-preact": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-remark-autolink-headers": "^2.3.3",
    "gatsby-remark-copy-linked-files": "^2.3.3",
    "gatsby-remark-custom-blocks": "^2.3.2",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-remark-images-anywhere": "^1.2.1",
    "gatsby-remark-prismjs": "^3.5.2",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
